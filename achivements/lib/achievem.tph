			EXTEND_TOP ~AR9603.bcs~ ~ACHIEVES.BAF~ EVALUATE_BUFFER
		END
	DEFAULT
END

/* months */
OUTER_SPRINT ~month_table~ ~zzMonths = {}~
COPY_EXISTING ~months.2da~ ~override~
	COUNT_2DA_ROWS 3 rows
	FOR ( i = 0 ; i < rows ; ++i ) BEGIN
		SET num = i + 1
		READ_2DA_ENTRY i 1 3 days
		READ_2DA_ENTRY i 2 3 month
		SPRINT ~month_table~ ~%month_table%%LNL%table.insert(zzMonths, {%num%, %days%, %month%})~
	END
	BUT_ONLY_IF_IT_CHANGES

/* options */
COPY_EXISTING ~ui.menu~ ~override~
	LPF FC_FIND_UI_TABLE
		STR_VAR searchtabl = "toggleTitles"
		RET offset_start offset_end offset_open numpairs
		RET_ARRAY numpairs
	END
	PATCH_IF ( offset_start >= 0 ) AND ( offset_end >= 0 ) BEGIN
		SPRINT line ~{"FC_ACHIEVEM_LABEL", "FC_ACHIEVEM_DESC", -403, 0, 0},%LNL%{"FC_ACHIEVEM_ACTIVE_LABEL", "FC_ACHIEVEM_ACTIVE_DESC", -404, 0, 0},%LNL%{"FC_ACHIEVEM_NODONE_LABEL", "FC_ACHIEVEM_NODONE_DESC", -405, 0, 0},~
		//SET linelength = STRING_LENGTH ~%line%~
		PATCH_IF ( numpairs = 0 ) BEGIN
			SET ins = offset_open + 1
			SPRINT line ~%LNL%%line%%LNL%~
			SET linelength = STRING_LENGTH ~%line%~
			INSERT_BYTES ins linelength
			WRITE_ASCIIE ins ~%line%~ (linelength)
		END ELSE BEGIN
			SET nprs = numpairs - 1
			SET offs = $numpairs(~%nprs%~)
			SET gzpt = INDEX_BUFFER(~,~ offs)
			PATCH_IF ( gzpt < offset_end ) BEGIN SPRINT zpt ~~ END ELSE BEGIN SPRINT zpt ~,~ END
			SET ins = offset_end - 1
			SPRINT line ~%zpt%%line%%LNL%~
			SET linelength = STRING_LENGTH ~%line%~
			INSERT_BYTES ins linelength
			WRITE_ASCIIE ins ~%line%~ (linelength)
		END
	END
	LPF FC_FIND_UI_MENU
		STR_VAR
			menu_name = "OPTIONS_GAMEPLAY"
		RET
			menu_text      //chargen_race_start
			menu_menu = menu_text_edit
			menu_success = success
			menu_menu_start = offset_start   //menu_menu_start
			menu_menu_end = offset_end     //menu_menu_end
	END
	PATCH_IF menu_success BEGIN
		TEXT_SPRINT insstr ~if (var[3] == -403) or (var[3] == -404) or (var[3] == -405) then
				var[5] = zZgetAchINI(var[3])
			else~
		LPF FC_PATCH_MENU_OPEN_CLOSE
			INT_VAR insert = 0
			STR_VAR
				menu_text = EVAL "%menu_menu%"
				edit_string = EVAL "%insstr%"
				add_to = EVAL "var\[5\][ %TAB%]*=[ %TAB%]*Infinity_GetOption(var\[3\],[ %TAB%]*panelID)"
				add_toz = "var[5] = Infinity_GetOption(var[3], panelID)"
			RET success menu_menu = return_text
		END
		LPF FC_PATCH_MENU_OPEN_CLOSE
			INT_VAR insert = 1
			STR_VAR
				menu_text = EVAL "%menu_menu%"
				edit_string = "end"
				add_to = EVAL "var\[5\][ %TAB%]*=[ %TAB%]*Infinity_GetOption(var\[3\],[ %TAB%]*panelID)"
				add_toz = "var[5] = Infinity_GetOption(var[3], panelID)"
			RET success menu_menu = return_text
		END
		LPF FC_FIND_UI_MENU_ELEMENT_EX
			STR_VAR menu_text = EVAL "%menu_menu%" element_name = "list" search_str = "toggleTitles"
			RET element_text success offset_start offset_end
		END
		PATCH_IF success BEGIN
			LPF FC_REPLACE_ELEMENT_ACTION
				INT_VAR
					replace = 0
					position = 1
					edge = 1
				STR_VAR
					element_text = EVAL "%element_text%"
					action_type = "action"
					action_string = "zZupdAchVal(selectedOpt)"
					//action_check = "end" //"highlightButtonToggle"
				RET
					element_text
			END
			LPM FC_UPDATE_MENU_ELEMENT
		END
		LPF FC_FIND_UI_MENU_ELEMENT_EX
			STR_VAR menu_text = EVAL "%menu_menu%" element_name = "button" search_str = "DONE_BUTTON"
			RET element_text success offset_start offset_end
		END
		PATCH_IF success BEGIN
			TEXT_SPRINT insstr ~if (var[3] == -403) or (var[3] == -404) or (var[3] == -405) then
				var[5] = zZsetAchINI(var[3])
			else~
			LPF FC_REPLACE_ELEMENT_ACTION
				INT_VAR
					replace = 0
					position = 0
				STR_VAR
					element_text = EVAL "%element_text%"
					action_type = "action"
					action_string = EVAL "%insstr%"
					add_to = EVAL "Infinity_ChangeOption([ %TAB%]*var\[3\],[ %TAB%]*var\[5\],[ %TAB%]*panelID)"
					add_toz = "Infinity_ChangeOption( var[3], var[5], panelID)"
				RET
					element_text
			END
			LPM FC_UPDATE_MENU_ELEMENT
		END
		LPF FC_FIND_UI_MENU_ELEMENT_EX
			STR_VAR menu_text = EVAL "%menu_menu%" element_name = "button" search_str = "DONE_BUTTON"
			RET element_text success offset_start offset_end
		END
		PATCH_IF success BEGIN
			LPF FC_REPLACE_ELEMENT_ACTION
				INT_VAR
					replace = 0
					position = 1
				STR_VAR
					element_text = EVAL "%element_text%"
					action_type = "action"
					action_string = "end"
					add_to = EVAL "Infinity_ChangeOption([ %TAB%]*var\[3\],[ %TAB%]*var\[5\],[ %TAB%]*panelID)"
					add_toz = "Infinity_ChangeOption( var[3], var[5], panelID)"
				RET
					element_text
			END
			LPM FC_UPDATE_MENU_ELEMENT
		END
	END
	LPF FC_RESTORE_MENU_COMMENTS STR_VAR menu_text = EVAL "%menu_menu%" RET menu_menu = menu_text END
	PATCH_IF menu_success BEGIN
		SET len = STRING_LENGTH ~%menu_menu%~
		DELETE_BYTES menu_menu_start ( menu_menu_end - menu_menu_start )
		INSERT_BYTES menu_menu_start len
		WRITE_ASCIIE menu_menu_start ~%menu_menu%~
	END















