DEFINE_PATCH_FUNCTION GET_BIT
INT_VAR
	number = 0
	nth_bit = 0
RET
	bit_value
BEGIN
	SET bit_value = (number >> nth_bit) & 1
END
DEFINE_ACTION_FUNCTION get_check_string
	STR_VAR var = ""
	RET scheck variable
BEGIN
	OUTER_SPRINT scheck "_CHECK"
	OUTER_SPRINT variable ~%var%~
	OUTER_SET slen = STRING_LENGTH ~%variable%~
	OUTER_SET schk = STRING_LENGTH ~%scheck%~
	OUTER_SET lenall = slen + schk
	OUTER_WHILE ( lenall > 32 ) AND ( schk > 1 ) BEGIN
		OUTER_SET schk = schk - 1
		OUTER_SNPRINT schk scheck ~%scheck%~
		OUTER_SET schk = STRING_LENGTH ~%scheck%~
		OUTER_SET lenall = slen + schk
	END
	ACTION_IF ( lenall > 32 ) BEGIN
		OUTER_WHILE ( lenall > 32 ) AND ( slen > 1 ) BEGIN
			OUTER_SET slen = slen - 1
			OUTER_SNPRINT slen variable ~%variable%~
			OUTER_SET slen = STRING_LENGTH ~%variable%~
			OUTER_SET lenall = slen + schk
		END
	END
END
DEFINE_ACTION_FUNCTION get_eet_var
	INT_VAR bit = 0 val = 0
	STR_VAR var = "" suff = ""
	RET eet_var
BEGIN
	OUTER_SPRINT eet_var ~~
	ACTION_IF bit BEGIN
		OUTER_SPRINT eet_var ~Global("%var%%suff%","GLOBAL",%val%)~
	END
END
DEFINE_ACTION_FUNCTION TO_DEC_NUMBER
STR_VAR
  hexNumber  = ~~
RET
  value
BEGIN
	OUTER_SET value = "-1"
	ACTION_IF (~%hexNumber%~ STRING_EQUAL ~~) BEGIN
		OUTER_SET value = 0
	END ELSE BEGIN
		ACTION_IF (~%hexNumber%~ STRING_MATCHES_REGEXP ~-.+~ = 0) BEGIN
			OUTER_SET signed = 1
			OUTER_PATCH_SAVE hexNumber ~%hexNumber%~ BEGIN REPLACE_TEXTUALLY ~^-~ ~~ END
		END ELSE BEGIN
			OUTER_SET signed = 0
		END
		ACTION_IF (~%hexNumber%~ STRING_MATCHES_REGEXP ~^0[xX].+~ = 0) BEGIN
			OUTER_PATCH_SAVE hexNumber ~%hexNumber%~ BEGIN REPLACE_TEXTUALLY ~^0[xX]~ ~~ END
		END
		OUTER_SET curValue = 0
		OUTER_SET scale = 1
		OUTER_SET strlen = STRING_LENGTH ~%hexNumber%~
		OUTER_FOR (idx = strlen - 1; idx >= 0; --idx) BEGIN
			OUTER_PATCH ~%hexNumber%~ BEGIN READ_BYTE idx hexDigit END
			OUTER_SET hexDigit = hexDigit BAND 0xff
			ACTION_IF (hexDigit >= 48 AND hexDigit <= 57) BEGIN // 0..9
				OUTER_SET digit = hexDigit - 48
			END ELSE ACTION_IF (hexDigit >= 65 AND hexDigit <= 70) BEGIN  // A..F
				OUTER_SET digit = hexDigit - 55
			END ELSE ACTION_IF (hexDigit >= 97 AND hexDigit <= 102) BEGIN // a..f
				OUTER_SET digit = hexDigit - 87
			END ELSE BEGIN
				OUTER_SET digit = 0
			END
			OUTER_SET curValue += digit*scale
			OUTER_SET scale = scale BLSL 4
		END
		ACTION_IF (signed) BEGIN
		  OUTER_SET curValue = 0 - curValue
		END
		OUTER_SET value = curValue
	END
END
DEFINE_PATCH_FUNCTION TO_DEC_NUMBER
	STR_VAR
		hexNumber  = ~~
	RET
		value
BEGIN
	INNER_ACTION BEGIN
		LAF TO_DEC_NUMBER STR_VAR hexNumber = EVAL ~%hexNumber%~ RET value END
	END
END
DEFINE_PATCH_FUNCTION CONCAT_BITS
	INT_VAR
		bit = 0
		add_bit = 0
		nthbit = 32
	RET
		value
BEGIN
	SET value = bit
	FOR ( i = 0; i < nthbit ; ++i ) BEGIN
		SET old = 0
		SET new = 0
		LPF GET_BIT INT_VAR number = bit nth_bit = i RET old = bit_value END
		LPF GET_BIT INT_VAR number = add_bit nth_bit = i RET new = bit_value END
		PATCH_IF ( NOT old AND new ) BEGIN SET value = value + 2 ** i END
	END
END

